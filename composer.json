{
    "name": "yiisoft/yii-debug",
    "type": "library",
    "description": "Yii Framework Debug Panel Extension",
    "keywords": [
        "yii",
        "debug",
        "debugger"
    ],
    "license": "BSD-3-Clause",
    "support": {
        "source": "https://github.com/yiisoft/yii-debug",
        "issues": "https://github.com/yiisoft/yii-debug/issues",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.0",
        "ext-mbstring": "*",
        "psr/log": "^1.1",
        "yiisoft/aliases": "^1.1|^2.0",
        "yiisoft/arrays": "^2.0",
        "yiisoft/assets": "^2.0",
        "yiisoft/di": "^1.0",
        "yiisoft/json": "^1.0",
        "yiisoft/profiler": "^1.0",
        "yiisoft/proxy": "dev-master",
        "yiisoft/strings": "^2.0",
        "yiisoft/var-dumper": "^1.0",
        "yiisoft/view": "^4.0",
        "yiisoft/yii-console": "^1.0",
        "yiisoft/yii-filesystem": "dev-master",
        "yiisoft/yii-http": "dev-master"
    },
    "require-dev": {
        "nyholm/psr7": "^1.3",
        "phpunit/phpunit": "^9.4",
        "roave/infection-static-analysis-plugin": "^1.5",
        "spatie/phpunit-watcher": "^1.23",
        "vimeo/psalm": "^4.2",
        "yiisoft/cache": "^1.0",
        "yiisoft/event-dispatcher": "^1.0",
        "yiisoft/log": "^1.0",
        "yiisoft/router-fastroute": "^3.0@dev"
    },
    "autoload": {
        "psr-4": {
            "Yiisoft\\Yii\\Debug\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yiisoft\\Yii\\Debug\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        },
        "config-plugin-options": {
            "source-directory": "config"
        },
        "config-plugin": {
            "tests": "tests.php",
            "params": "params.php",
            "common": "common.php",
            "console": "console.php",
            "web": "web.php",
            "providers": "providers.php",
            "events-web": "events-web.php",
            "events-console": "events-console.php"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": "phpunit --testdox --no-interaction",
        "test-watch": "phpunit-watcher watch"
    }
}
