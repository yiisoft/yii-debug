{
    "name": "yiisoft/yii-debug",
    "type": "library",
    "description": "Yii Framework Debug Panel Extension",
    "keywords": ["yii", "debug", "debugger"],
    "license": "BSD-3-Clause",
    "support": {
        "source": "https://github.com/yiisoft/yii-debug",
        "issues": "https://github.com/yiisoft/yii-debug/issues",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii"
    },
    "authors": [
        {
            "name": "Qiang Xue",
            "email": "qiang.xue@gmail.com"
        }
    ],
    "require": {
        "yiisoft/yii-core": "^3.0@dev",
        "yiisoft/yii-dataview": "^3.0@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.3",
        "yiisoft/yii-bootstrap4": "^3.0@dev",
        "yiisoft/yii-web": "^3.0@dev",
        "yiisoft/di": "^3.0@dev",
        "yiisoft/log": "^3.0@dev",
        "yiisoft/view": "^3.0@dev",
        "yiisoft/cache": "^3.0@dev",
        "hiqdev/composer-config-plugin": "^1.0@dev"
    },
    "autoload": {
        "psr-4": {
            "yii\\debug\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "yiiunit\\debug\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        },
        "config-plugin": {
            "params": "config/params.php",
            "web": "config/web.php"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
