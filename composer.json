{
    "name": "yiisoft/yii-debug",
    "type": "library",
    "description": "Yii Framework Debug Panel Extension",
    "keywords": [
        "yii",
        "debug",
        "debugger"
    ],
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/yii-debug/issues?state=open",
        "forum": "https://www.yiiframework.com/forum/",
        "wiki": "https://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "chat": "https://t.me/yii3en",
        "source": "https://github.com/yiisoft/yii-debug"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.0",
        "ext-mbstring": "*",
        "jetbrains/phpstorm-attributes": "^1.0",
        "psr/log": "^2.0|^3.0",
        "yiisoft/aliases": "^1.1|^2.0",
        "yiisoft/arrays": "^2.0",
        "yiisoft/assets": "^2.0.1",
        "yiisoft/di": "^1.0",
        "yiisoft/json": "^1.0",
        "yiisoft/profiler": "^1.0.3",
        "yiisoft/proxy": "^1.0.1",
        "yiisoft/strings": "^2.0",
        "yiisoft/validator": "3.0.x-dev",
        "yiisoft/var-dumper": "^1.0",
        "yiisoft/view": "^4.0|^5.0|^6.0",
        "yiisoft/yii-console": "^1.0",
        "yiisoft/yii-filesystem": "dev-master",
        "yiisoft/yii-http": "^1.0",
        "yiisoft/yii-queue": "3.0.x-dev"
    },
    "require-dev": {
        "nyholm/psr7": "^1.3",
        "phpunit/phpunit": "^9.4",
        "roave/infection-static-analysis-plugin": "^1.16",
        "spatie/phpunit-watcher": "^1.23",
        "vimeo/psalm": "^4.18",
        "yiisoft/event-dispatcher": "^1.0",
        "yiisoft/log": "^2.0",
        "yiisoft/router-fastroute": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Yiisoft\\Yii\\Debug\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yiisoft\\Yii\\Debug\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        },
        "config-plugin-options": {
            "source-directory": "config"
        },
        "config-plugin": {
            "params": "params.php",
            "common": "common.php",
            "console": "console.php",
            "web": "web.php",
            "providers": "providers.php",
            "events-web": "events-web.php",
            "events-console": "events-console.php"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "infection/extension-installer": true,
            "composer/package-versions-deprecated": true
        }
    },
    "scripts": {
        "test": "phpunit --testdox --no-interaction",
        "test-watch": "phpunit-watcher watch"
    }
}
