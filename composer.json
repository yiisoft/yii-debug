{
    "name": "yiisoft/yii-debug",
    "type": "library",
    "description": "Yii Framework Debug Panel Extension",
    "keywords": [
        "yii",
        "debug",
        "debugger"
    ],
    "license": "BSD-3-Clause",
    "support": {
        "source": "https://github.com/yiisoft/yii-debug",
        "issues": "https://github.com/yiisoft/yii-debug/issues",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "yiisoft/arrays": "^3.0@dev",
        "yiisoft/strings": "^3.0@dev",
        "yiisoft/profiler": "dev-master",
        "yiisoft/assets": "dev-master",
        "yiisoft/view": "^3.0@dev",
        "psr/log": "^1.1",
        "yiisoft/yii-web": "^3.0@dev",
        "ext-mbstring": "*",
        "php": "^7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "yiisoft/di": "^3.0@dev",
        "yiisoft/log": "^3.0@dev",
        "yiisoft/cache": "^3.0@dev",
        "hiqdev/composer-config-plugin": "^1.0@dev",
        "nyholm/psr7": "^1.0",
        "yiisoft/event-dispatcher": "^3.0@dev"
    },
    "autoload": {
        "psr-4": {
            "Yiisoft\\Yii\\Debug\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yiisoft\\Yii\\Debug\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        },
        "config-plugin": {
            "tests": "config/tests.php",
            "params": "config/params.php",
            "web": "config/web.php"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "config": {
        "sort-packages": true
    }
}
